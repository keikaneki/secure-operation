/** Footer download **/
.ft-download .ft-download_inner {border:1px solid #ddd; width:30%; padding:15px 10px;}

.ft-download .ft-download_inner .ft-download_inner_l,
.ft-download .ft-download_inner .ft-download_inner_l2 {float:left; width:110px; }

.ft-download .ft-download_inner .ft-download_inner_l img {width:110px; position:absolute; transform: translateY(15%);}

.ft-download .ft-download_inner .ft-download_inner_l2 img {width:110px; position:absolute; transform: translateY(-34%);}

.ft-download .ft-download_inner .ft-download_inner_r,
.ft-download .ft-download_inner .ft-download_inner_r2 {float:right;}

.ft-download .ft-download_inner .ft-download_inner_r .txt,
.ft-download .ft-download_inner .ft-download_inner_r2 .txt {font-size:13px; line-height:1.4; padding-bottom:.5rem;}

@media screen and (max-width: 960px) {
	.ft-download .ft-download_inner .ft-download_inner_l img{float:none; width:150px; margin:0 auto; }

	.ft-download .ft-download_inner .ft-download_inner_l {float:none; width:160px; margin:0 auto;}
	.ft-download .ft-download_inner .ft-download_inner_l2 {float:none; width:110px; margin:0 auto;}

	.ft-download .ft-download_inner .ft-download_inner_r,
	.ft-download .ft-download_inner .ft-download_inner_r2 {float:none; text-align:center; bottom:0;}

	.ft-download .ft-download_inner .ft-download_inner_l2 img {width:110px; position:relative; transform: translateY(0%); margin:0 auto;}
}

@media screen and (max-width: 768px) {
	.ft-download .ft-download_inner {overflow:hidden; width:100%; margin:0 0 35px; padding:15px 20px 15px 10px;}
	.ft-download .ft-download_inner .ft-download_inner_l,
	.ft-download .ft-download_inner .ft-download_inner_l2 {float:left; width:110px; }

	.ft-download .ft-download_inner .ft-download_inner_l img {width:100%; }

	.ft-download .ft-download_inner .ft-download_inner_l2 img {width:110px; position:absolute; transform: translateY(-30%);}

	.ft-download .ft-download_inner .ft-download_inner_r,
	.ft-download .ft-download_inner .ft-download_inner_r2 {float:right;}

	.ft-download .ft-download_inner .ft-download_inner_r .txt,
	.ft-download .ft-download_inner .ft-download_inner_r2 .txt {font-size:15px; line-height:1.4; padding-bottom:.5rem; display: inline-block;
		text-align: left;
		margin: 0 20px 0 0;}
	.ft-download .ft-download_inner .ft-download_inner_r .txt br,
	.ft-download .ft-download_inner .ft-download_inner_r2 .txt br {display:none;}
}

@media screen and (max-width: 640px) {
	.ft-download .ft-download_inner {border:1px solid #ddd; width:94%; padding:15px 10px;}

	.ft-download .ft-download_inner .ft-download_inner_l,
	.ft-download .ft-download_inner .ft-download_inner_l2 {float:left; width:110px; }

	.ft-download .ft-download_inner .ft-download_inner_l img {width:110px; }

	.ft-download .ft-download_inner .ft-download_inner_l2 img {width:110px; position:absolute; transform: translateY(-30%);}

	.ft-download .ft-download_inner .ft-download_inner_r,
	.ft-download .ft-download_inner .ft-download_inner_r2 {float:right; margin-right:15px;}

