/* index bottun window */
.killer {
position: relative;
min-height: 80%;

margin-bottom: 0px;
display: flex;
flex-direction: column;
justify-content: space-between;
}

.killer .movie .top_mainimg img {
max-width: 1400px;
margin:0 auto;
}

.killer::before {
content: none;
position: absolute;
left: 0;
top: 0;
width: 100%;
height: 100%;
background: rgba(0, 0, 0, 0.5);
}

.killer .bg-img {
transform: skewY(-14deg);
transform-origin: 680px top 0;
overflow: hidden;
position: absolute;
left: 0;
top: 0;
width: 100%;
z-index: 10;
display: none;
}

.killer .bg-img div {
height: 620px;
margin-top: 200px;
transform: skewY(14deg);
transform-origin: 680px top 0;
}

.killer .bg-img div img {
max-width: none;
width: 100%;
position: relative;
right: 0;
top: -100px;
}

.killer .ai_dl {
z-index: 11;
transform: translateY(-200%);
}

.killer .ai_dl .inner {
height: auto;
margin: 0 auto;
box-shadow: 0 5px 20px rgba(0, 0, 0, 0.1);
padding: 10px;

background: #5db2efd9;
/* Old browsers */
background: -moz-linear-gradient(left, #5db2efd9 0%, #90fbbade 100%);
/* FF3.6-15 */
background: -webkit-linear-gradient(left, #5db2efd9 0%, #90fbbade 100%);
/* Chrome10-25,Safari5.1-6 */
background: linear-gradient(to right, #5db2efd9 0%, #90fbbade 100%);
/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#5db2efd9', endColorstr='#90fbbade', GradientType=1);
/* IE6-9 */
}

.killer .ai_dl figure {
width: 110px;
position: relative;
margin-right: 10px;
}

.killer .ai_dl figure img {
width: 100%;
float: left;
position: absolute;
left: 0;
bottom: -65px;
}

.killer .ai_dl p {
margin-bottom: -10px;
text-align: left;
line-height: 1.4;
margin-right: 0px;
font-size: .8rem;
color: #fff;
padding-left: 120px;
-moz-text-shadow: 1px -1px 4px rgba(0, 0, 0, 0.27);
-webkit-text-shadow: 1px -1px 4px rgba(0, 0, 0, 0.27);
-ms-text-shadow: 1px -1px 4px rgba(0, 0, 0, 0.27);
text-shadow: 1px -1px 4px rgba(0, 0, 0, 0.27);
}

.killer .ai_dl .ai_dl_title {
clear: both;
overflow: hidden;
margin: -.3rem 0 .5em 120px;
}

.killer .ai_dl .ai_dl_title p {
display: block;
font-size: 1.2rem;
font-weight: bold;
text-align:left;
color: #fff;
margin: 10px 0 0 0;
padding: 0;
text-align: center;
-moz-text-shadow: 1px -1px 4px rgba(0, 0, 0, 0.27);
-webkit-text-shadow: 1px -1px 4px rgba(0, 0, 0, 0.27);
-ms-text-shadow: 1px -1px 4px rgba(0, 0, 0, 0.27);
text-shadow: 1px -1px 4px rgba(0, 0, 0, 0.27);
}

.killer .ai_dl_btn {
text-align: center;
display: inline-flex;
justify-content: center;
align-items: center;
height: 36px;
width:100%;
color: #75bbf0;

background: #fff;

box-shadow: 0 3px 6px rgba(0, 0, 0, 0.15);
margin: 0px auto;
font-size:.9rem;
}	

.killer .ai_dl_btn img {
width: 16px;
margin-right: .5rem;
}

.killer .ai_dl_btn a {
color: #75bbf0
}

.killer .ai_dl_btn span {
transform: translateY(0px);
}

.killer .ai_dl_btn:hover {
box-shadow: 0 10px 20px rgba(0, 0, 0, 0.15);
filter: brightness(110%);
}


