/* case-pager
	========================================================================== */
.case-pager {
margin-bottom: 20px;
width: 98vw;
position: relative;
left: 50%;
transform: translateX(-50%);
background: #fff;
border-top: 1px solid #7f7f80;
border-bottom: 1px solid #7f7f80;
text-align: center;
}

.case-pager ul {
margin: auto;
display: flex;
flex-direction: row;
justify-content: center;
width: 100%;
max-width: 1000px;
}

.case-pager ul li {
position: relative;
display: flex;
justify-content: center;
align-items: center;
}

.case-pager ul li a {
position: relative;
display: block;
padding: 10px 40px;
font-size: 14px;
background: #fff;
opacity: 1.0;
}

.case-pager ul li.home {
width: 45%;
border-left: 1px solid #7f7f80;
border-right: 1px solid #7f7f80;
line-height: 1.6;
}

.case-pager ul li.home img {
margin: auto;
margin-right: 10px;
width: 20px;
height: 19px;
}

.case-pager ul li.prev,
.case-pager ul li.next {
width: 35%;
}

.case-pager ul li.prev {
text-align: right;
}

.case-pager ul li.next {
text-align: left;
}

.case-pager ul li.next::after,
.case-pager ul li.prev::before {
content: "";
display: block;
position: absolute;
right: 20px;
top: 50%;
width: 9px;
height: 18px;
transform: translateY(-50%);
background-image: url("../img/ico/arrow.png");
background-repeat: no-repeat;
background-position: center right;
background-size: 9px auto;
z-index: 1;
}

.case-pager ul li:empty.next::after,
.case-pager ul li:empty.prev::before {
display: none;
}

.case-pager ul li.prev {
transform: rotateZ(180deg);
}

.case-pager ul li.next a::after,
.case-pager ul li.prev a::before {
display: block;
position: absolute;
right: 20px;
top: 50%;
width: 9px;
height: 18px;
transform: translateY(-50%);
background-image: url("../img/ico/arrow.png");
background-repeat: no-repeat;
background-position: center right;
background-size: 9px auto;
}

.case-pager ul li.prev a span {
transform: rotateZ(180deg);
}

@media screen and (max-width: 900px) {
.case-pager ul li a {
	padding: 10px 20px;
	font-size: 12px;
}
}

@media screen and (max-width: 640px) {
.case-pager ul li a {
	padding: 10px 15px 10px 0;
	font-size: 12px;
}

.case-pager ul li.next::after,
.case-pager ul li.prev::before {
	right: 15px;
	width: 6px;
	height: 12px;
	background-size: 6px auto;
}

.case-pager ul li.next a::after,
.case-pager ul li.prev a::before {
