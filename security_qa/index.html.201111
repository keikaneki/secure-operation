	<header class="header-include"></header>
	<section class="l-inner security_qa_page ofxh">
		<nav class="breadcrumb">
			<ul>
				<li><a href="/">TOP</a></li>
				<li><span>よくある質問</span></li>
			</ul>
		</nav>

		<div class="ttlarea ta-c">
			<h1 class="fs-rennermedium fs-50">Q&amp;A</h1>
			<h2 class="fs-20">よくある質問</h2>
		</div>

		<div class="ttlarea ta-c">
			<h3 class="blackbelt">みなさまから寄せられる質問とその回答をご紹介</h3>
		</div>

		<div class="block">
			<p class="fs-30">調べたいキーワードから検索</p>
			<div class="searchbox">
				<input type="text" v-model="search_word" @keydown.enter="faq_search_words">
				<button @click="faq_search_words">検索</button>
			</div>
		</div>

		<div class="block" v-if="category_data['keywords']">
			<div class="subttlarea">
				<h4 class="orangeunderline fs-30">注目のキーワード</h4>
			</div>
			<ul class="rowlist">
				<li v-for="(keyword,index) in category_data['keywords']">
					<a :href="'/security_qa/search.html?tag=' + keyword['term_id']" class="btn black xs">{{ keyword['name'] }}</a>
				</li>
			</ul>
		</div>

		<div class="block" v-if="faq_data['recommend_question']">
			<div class="subttlarea">
				<div class="orangeunderline"><span class="fs-30">お問い合わせの多い質問</span></div>
			</div>
			<div class="qa" v-for="(recommend_question,index) in faq_data['recommend_question']">
				<div class="q">
					<a :href="'/security_qa/detail.html?id=' + recommend_question['ID']"><span>{{ recommend_question['post_title'] }}</span></a>
				</div>
				<div class="a">
					<div class="indent">{{ recommend_question['answer'] }}</div>
				</div>
			</div>
		</div>

		<div class="block" v-if="category_data['term']['service']">
			<div class="subttlarea">
				<h4 class="orangeunderline fs-30">サービスから探す</h4>
			</div>
			<ul class="l-cols2">
				<li class="col p-10" v-for="(keyword,index) in category_data['term']['service']">
					<a :href="'/security_qa/search.html?cat=' + keyword['term_id']" class="btn long gray belt"><span>{{ keyword['name'] }}</span></a>
				</li>
			</ul>
		</div>

		<div class="block" v-if="category_data['term']['industry']">
			<div class="subttlarea">
				<h4 class="orangeunderline fs-30">業種から探す</h4>
			</div>
			<ul class="l-cols2">
				<li class="col p-10" v-for="(keyword,index) in category_data['term']['industry']">
					<a :href="'/security_qa/search.html?cat=' + keyword['term_id']" class="btn long gray belt"><span>{{ keyword['name'] }}</span></a>
				</li>
			</ul>
		</div>
